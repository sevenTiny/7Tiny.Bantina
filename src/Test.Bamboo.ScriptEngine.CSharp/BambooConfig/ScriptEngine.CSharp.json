/* CSharp脚本引擎配置文件示范 */
{
  //Nuget源，配置在根的源会应用到所有的应用节点
  "NugetSources": [
    "https://api.nuget.org/v3/index.json"
  ],
  // 是否DebugMode编译，默认Release模式编译
  "IsDebugModeCompile": false,
  // 输出编译文件
  "IsOutPutCompileFiles": false,
  // dll扫描并全部加载的路径，该目录下的dll会全部加载
  "DllScanAndLoadPath": [
    "./"
  ],
  // 需要下载的包
  "InstallPackages": [
    {
      // nuget包Id
      "PackageId": "Newtonsoft.Json",
      // 包版本
      "Version": "13.0.3"
    }
  ]
}
